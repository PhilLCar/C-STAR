{
    "name": "c-star",
    "displayName": "C-STAR",
    "description": "Language support for C*",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.45.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "csr",
            "aliases": ["C*", "csr"],
            "extensions": [".csr",".hsr",".psr"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "csr",
            "scopeName": "source.csr",
            "path": "./syntaxes/csr.tmLanguage.json"
        }],
        "iconThemes": [{
            "id": "csr-icon",
            "label": "C-STAR",
            "path": "./fileicons/csr-icon-theme.json"
        }]
    }
}