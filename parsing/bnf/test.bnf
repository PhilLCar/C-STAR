#;; include (parsing/bnf/numbers.bnf)

<op> ::= <oct> { ("+" | "-") <oct> }+ "E"
<oct> ::=  [ "-" ] ( "0" | "0o" ), <oct-digit-no-zero>, { <oct-digit> }
<oct-digit-no-zero> ::= '1' |
                        '2' |
                        '1b' |
                        '4' |
                        '5' |
                        '6' |
                        '7'
<oct-digit> ::= <oct-digit-no-zero> | '0'